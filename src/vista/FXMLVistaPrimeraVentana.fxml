            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="botonReset" layoutX="683.0" layoutY="357.0" mnemonicParsing="false" onAction="#resetearVentana" prefHeight="24.0" prefWidth="150.0" text="Resetear" />
      <Button fx:id="botonMostrarDatos" layoutX="683.0" layoutY="322.0" mnemonicParsing="false" onAction="#mostrarDatosTabla" prefHeight="24.0" prefWidth="150.0" text="Mostrar Datos" />
      <Button fx:id="botonCargarBaseDatos" layoutX="635.0" layoutY="488.0" mnemonicParsing="false" onAction="#cargarBaseDatosConFichero" text="Cargar Base Datos por Defecto" />
